{
  "VasCruz Component Template": {
    "prefix": "vzcomp",
    "body": [
      "\"use client\";",
      "",
      "import { motion } from \"framer-motion\";",
      "import { $1 } from \"lucide-react\";",
      "import { $2 } from \"@/components/ui/$3\";",
      "import { useMessages } from \"next-intl\";",
      "",
      "// Icon mapping",
      "const iconMapping = {",
      "  $4: $5,",
      "};",
      "",
      "export default function $6() {",
      "  const messages = useMessages();",
      "",
      "  // Extract data from messages",
      "  const data = (messages as any)?.$7?.$8;",
      "",
      "  // Fallback",
      "  if (!data) {",
      "    return (",
      "      <section className=\"$9\">",
      "        <div className=\"container mx-auto px-4\">",
      "          <div className=\"text-center\">",
      "            <p className=\"text-muted-foreground\">Content not available</p>",
      "          </div>",
      "        </div>",
      "      </section>",
      "    );",
      "  }",
      "",
      "  return (",
      "    <section className=\"$9\">",
      "      <div className=\"container mx-auto px-4\">",
      "        {/* Content */}",
      "      </div>",
      "    </section>",
      "  );",
      "}"
    ],
    "description": "Base template for VasCruz Group components"
  },
  "Legal Page Template": {
    "prefix": "legalpage",
    "body": [
      "\"use client\";",
      "",
      "import { motion } from \"framer-motion\";",
      "import { useMessages } from \"next-intl\";",
      "",
      "export default function $1() {",
      "  const messages = useMessages();",
      "",
      "  // Extract data from messages",
      "  const data = (messages as any)?.legal?.$2;",
      "",
      "  // Fallback",
      "  if (!data) {",
      "    return (",
      "      <div className=\"min-h-screen bg-background\">",
      "        <div className=\"max-w-4xl mx-auto px-4 py-8 lg:py-12\">",
      "          <div className=\"text-center\">",
      "            <p className=\"text-muted-foreground\">Content not available</p>",
      "          </div>",
      "        </div>",
      "      </div>",
      "    );",
      "  }",
      "",
      "  return (",
      "    <div className=\"min-h-screen bg-background\">",
      "      {/* Page Banner */}",
      "      <section className=\"bg-primary text-primary-foreground py-8 lg:py-12\">",
      "        <div className=\"container mx-auto px-4\">",
      "          <motion.div",
      "            initial={{ opacity: 0, y: 20 }}",
      "            animate={{ opacity: 1, y: 0 }}",
      "            transition={{ duration: 0.6 }}",
      "            className=\"text-center\"",
      "          >",
      "            <h1 className=\"text-3xl lg:text-4xl font-bold text-primary-foreground\">",
      "              {data.title}",
      "            </h1>",
      "          </motion.div>",
      "        </div>",
      "      </section>",
      "",
      "      {/* Main Content */}",
      "      <section className=\"py-8 lg:py-12 bg-background\">",
      "        <div className=\"max-w-4xl mx-auto px-4\">",
      "          <motion.div",
      "            initial={{ opacity: 0, y: 40 }}",
      "            animate={{ opacity: 1, y: 0 }}",
      "            transition={{ duration: 0.8, delay: 0.2 }}",
      "          >",
      "            {/* Last Updated */}",
      "            {data.lastUpdated && (",
      "              <p className=\"text-lg text-muted-foreground mb-8\">",
      "                Última actualización: {data.lastUpdated}",
      "              </p>",
      "            )}",
      "            ",
      "            {/* Introduction */}",
      "            {data.introduction && (",
      "              <div className=\"mb-8\">",
      "                <p className=\"text-foreground leading-relaxed mb-4\">",
      "                  {data.introduction}",
      "                </p>",
      "              </div>",
      "            )}",
      "            ",
      "            {/* Content Sections */}",
      "            {data.sections?.map((section: any, idx: number) => (",
      "              <div key={idx} className=\"mb-8\">",
      "                <h2 className=\"text-xl lg:text-2xl font-semibold text-foreground mt-8 mb-4\">",
      "                  {section.title}",
      "                </h2>",
      "                {section.content && (",
      "                  <p className=\"text-foreground leading-relaxed mb-4\">",
      "                    {section.content}",
      "                  </p>",
      "                )}",
      "                {section.items && (",
      "                  <ul className=\"space-y-2 mb-6\">",
      "                    {section.items.map((item: string, itemIdx: number) => (",
      "                      <li key={itemIdx} className=\"text-foreground mb-2 flex items-start\">",
      "                        <span className=\"text-primary mr-2\">•</span>",
      "                        {item}",
      "                      </li>",
      "                    ))}",
      "                  </ul>",
      "                )}",
      "              </div>",
      "            ))}",
      "            ",
      "            {/* Contact Section */}",
      "            {data.contact && (",
      "              <div className=\"bg-muted/30 p-6 rounded-lg mt-8\">",
      "                <h2 className=\"text-xl lg:text-2xl font-semibold text-foreground mb-4\">",
      "                  {data.contact.title}",
      "                </h2>",
      "                <div className=\"space-y-2\">",
      "                  {data.contact.address && (",
      "                    <p className=\"text-foreground\">{data.contact.address}</p>",
      "                  )}",
      "                  {data.contact.email && (",
      "                    <p className=\"text-foreground\">Email: {data.contact.email}</p>",
      "                  )}",
      "                  {data.contact.phone && (",
      "                    <p className=\"text-foreground\">Teléfono: {data.contact.phone}</p>",
      "                  )}",
      "                </div>",
      "              </div>",
      "            )}",
      "          </motion.div>",
      "        </div>",
      "      </section>",
      "    </div>",
      "  );",
      "}"
    ],
    "description": "Template for legal pages with professional document structure"
  },
